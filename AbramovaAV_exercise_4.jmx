              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">def cities = [&quot;Denver&quot;, &quot;Frankfurt&quot;, &quot;London&quot;, &quot;Los Angeles&quot;, &quot;Paris&quot;, &quot;Portland&quot;, &quot;San Francisco&quot;, &quot;Seattle&quot;, &quot;Sydney&quot;, &quot;Zurich&quot;]

def getRandomCity(cities, excludeCity = null) {
    def randomCity
    do {
        randomCity = cities[new Random().nextInt(cities.size())]
    } while (randomCity == excludeCity) 
    return randomCity
}

def departureCity = getRandomCity(cities)
def arrivalCity = getRandomCity(cities, departureCity)

vars.put(&quot;departure_city&quot;, departureCity)
vars.put(&quot;arrival_city&quot;, arrivalCity)</stringProp>
