              <stringProp name="parameters"></stringProp>
              <stringProp name="script">import org.apache.commons.lang3.RandomStringUtils;

String generateRandomName() {
    String allowedChars = &apos;abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789@./+/_&apos;;
    return RandomStringUtils.random(10, allowedChars.toCharArray());
}

String generateRandomPassword() {
    String letters = &apos;abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ&apos;;
    String digits = &apos;0123456789&apos;;
    String specialChars = &apos;@./+/_&apos;;

    String part1 = RandomStringUtils.random(4, letters.toCharArray());
    String part2 = RandomStringUtils.random(2, digits.toCharArray());
    String part3 = RandomStringUtils.random(2, specialChars.toCharArray());

    String password = part1 + part2 + part3;
    password = RandomStringUtils.random(8, (letters + digits + specialChars).toCharArray());

    return password;
}

String random_user = generateRandomName();
String random_password = generateRandomPassword();

vars.put(&quot;randomName&quot;, random_user);
vars.put(&quot;randomPassword1&quot;, random_password);
vars.put(&quot;randomPassword2&quot;, random_password);


FileWriter fWriter = new FileWriter(&quot;exercise_2.csv&quot;, true);
String line = vars.get(&quot;randomName&quot;) + &apos;;&apos; + vars.get(&quot;randomPassword1&quot;) +&apos;;&apos; + vars.get(&quot;randomPassword2&quot;) + &apos;\n&apos;;
fWriter.write(line);
fWriter.close();

</stringProp>
